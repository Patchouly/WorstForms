<form id="form6" accept-charset="UTF-8" role="form" class="form6" action="/submit" method="post">
    <div class="row center-align">
        <div class="col s12 m4 offset-m4">
            <input id="indexPhoneNumber" type="text" disabled>
        </div>
    </div>
    <div class="row">
        <div class="col s12 m4 offset-m2">
            <input type="submit" id="form6submit" class="btn center waves-effect waves-light z-depth-4" value="Cadastrar"/>
        </div>
        <div class="col s12 m4">
            <input type="button" id="notMyNumber" class="btn center waves-effect waves-light z-depth-4 red" value="Não é meu número"/>
        </div>
    </div>
    <input id="phoneNumber" type="text" name="phoneNumber" class="hiddenPhone" value="<%=dadosForm.phoneNumber%>"/>
</form>